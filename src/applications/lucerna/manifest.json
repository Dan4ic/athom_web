{
  "name" : "Lucerna",
  "langs" : {
    "source" : "langs.js"
  },
  "store" : {
    "source" : "storage.js"
  },
  "components" : {
    "lucerna-app" : {
      "source" : "index.js",
      "intent_filter" : [
        {
          "action"    : "smarttank.intent.action.MAIN",
          "category"  : "smarttank.intent.category.LAUNCHER"
        }
      ]
    },
    "lucerna-scheduller" : {
      "source" : "index.js"
    },
    "lucerna-dot-inspector" : {
      "source" : "index.js"
    },
    "lucerna-preferences" : {
      "source" : "prefs.js",
      "intent_filter" : [
        {
          "action"    : "smarttank.intent.action.MAIN",
          "category"  : "smarttank.intent.category.PREFERENCE"
        }
      ]
    }
  }
}